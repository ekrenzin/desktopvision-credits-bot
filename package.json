    "itty-router": "^4.0.13",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "c8": "^8.0.0",
    "chai": "^5.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "mocha": "^10.2.0",
    "ngrok": "^5.0.0-beta.2",
    "prettier": "^3.0.1",
    "sinon": "^17.0.0",
    "wrangler": "^3.10.1"
